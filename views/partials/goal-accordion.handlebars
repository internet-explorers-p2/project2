<div class="container-fluid" id="accordion-area">
  <div class="card mt-3">
    <div class="card-header" id="headingOne">
      <h2 class="mb-0">
        <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#collapse{{dataValues.id}}"
          aria-expanded="true" aria-controls="collapse{{dataValues.id}}">
          {{dataValues.title}}
        </button>
      </h2>
      {{!-- PROGRESS BAR --}}
      <div class="progress" style="height: 2px;">
        <div class="progress-bar" role="progressbar" style="width: 25%;" aria-valuenow="25" aria-valuemin="0"
          aria-valuemax="100"></div>
      </div>
    </div>

    <div id="collapse{{dataValues.id}}" class="collapse show" aria-labelledby="headingOne"
      data-parent="#accordionExample">
      <div class="card-body overflow-auto">
        {{!-- {{> milestone-button}} --}}


        {{!-- MILESTONE LIST --}}
        <div id="myDIV" class="header">
          <h2 style="margin:5px" id="milestone-heading">Add a Milestone</h2>
          <input type="text" id="myInput" placeholder="Milestone...">
          <span onclick="newElement()" class="addBtn">Add</span>
        </div>

        <ul id="myUL">
          <li id="myLi">Hit the gym</li>
          <li id="myLi" class="checked">Pay bills</li>
          <li id="myLi">Meet George</li>
          <li id="myLi">Buy eggs</li>
          <li id="myLi">Read a book</li>
          <li id="myLi">Organize office</li>
        </ul>

        {{!-- <div id="progressbar"></div>

        <a id="plus">+</a>
        <a id="minus">-</a>
        <p id="progressLabel">0%</p> --}}
      </div>
    </div>
  </div>
</div>